# CORS Enabling
Header set Access-Control-Allow-Origin "*"
Header set Access-Control-Allow-Headers "*"
Header set Access-Control-Allow-Methods "GET, POST, PUT, DELETE, OPTIONS"
Header set X-Frame-Options SAMEORIGIN

#Header set Access-Control-Allow-Origin "*"

# Generated by FastPlaz

RewriteEngine On
AddHandler cgi-script .cgi
AddHandler cgi-script .bin
AddHandler cgi-script .pas
AddHandler cgi-script .exe
Options +ExecCGI

DirectoryIndex carik.bin index.html

SetEnvIf Authorization "(.*)" Authorization=$1
SetEnvIf ClientID "(.*)" ClientID=$1
SetEnvIf Token "(.*)" Token=$1
SetEnvIf DeviceID "(.*)" DeviceID=$1

SetEnvIf Authorization "(.*)" Authorization=$1

RewriteRule ^(line)$ line.bin/$1 [QSA,L]
RewriteRule ^(facebook)$ facebook.bin/$1 [QSA,L]
RewriteRule ^(fb)$ facebook.bin/$1 [QSA,L]
RewriteRule ^(telegram)$ telegram.bin/$1 [QSA,L]

#
RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -l
RewriteRule ^(.*)$ - [NC,L]
#RewriteRule ^(.*)$ 404.php [QSA,L]
RewriteRule ^(.*)$ carik.bin/$1 [QSA,L]

